{
    "name": "mauricerenck/podcaster",
    "version": "1.2.4",
    "description": "A Kirby Podcast Plugin",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurice Renck",
            "email": "hello@maurice-renck.de"
        }
    ],
    "autoload": {
        "files": [
            "utils/PodcasterUtils.php",
            "utils/PodcasterAudioUtils.php",
            "utils/PodcasterStats.php",
            "utils/PodcasterStatsFile.php",
            "utils/PodcasterStatsMysql.php",
            "utils/PodcasterStatsPodTrac.php",
            "utils/PodcasterWizard.php",
            "lib/PiwikTracker.php"
        ]
    },
    "require": {
        "getkirby/composer-installer": "^1.1",
        "james-heinrich/getid3": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "config": {
        "optimize-autoloader": true
    }
}
